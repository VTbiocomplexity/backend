{
  "name": "backend",
  "version": "0.0.2",
  "description": "backend",
  "main": "index.js",
  "engines": {
    "node": "8.11.1"
  },
  "license": "MIT",
  "scripts": {
    "installglobals": "npm install -g npm@latest",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install && npm run postinstallsh",
    "debug": "node --inspect-brk index.js",
    "mocha": "cross-env NODE_ENV=test mocha -r ./test/bootstrap.js test/**/*.js",
    "nyc": "nyc nyc --reporter=lcov --reporter=text-lcov npm run mocha",
    "test": "eslint . && npm run nyc",
    "test:lint": "eslint .",
    "test:debug": "cross-env NODE_ENV=test mocha debug -r ./test/bootstrap.js test/**/*.js",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "postinstallsh": "./postinstall.sh",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect && npm run postinstallsh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/VTbiocomplexity/backend.git"
  },
  "author": "VTbiocomplexity <>",
  "contributors": [{
      "name": ""
    },
    {
      "name": ""
    }
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-sslify": "^1.2.0",
    "helmet": "^3.9.0",
    "jwt-simple": "^0.5.0",
    "moment": "^2.15.2",
    "mongoose": "^4.10.0",
    "morgan": "^1.9.0",
    "node-rafter": "^1.0.1",
    "nodemailer": "^4.3.0",
    "request": "^2.83.0",
    "snyk": "^1.69.10"
  },
  "devDependencies": {
    "aurelia-tools": "^2.0.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "mocha": "3.1.2",
    "mockgoose": "^6.0.8",
    "nock": "^9.0.27",
    "nodemon": "^1.9.2",
    "nyc": "^11.3.0",
    "sinon": "^4.1.0"
  },
  "snyk": true
}