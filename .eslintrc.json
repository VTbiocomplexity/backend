{
  "extends": ["./node_modules/aurelia-tools/.eslintrc.json", "plugin:react/recommended", "airbnb"],
  "rules": {
    "no-console": [0, "never"],
    "comma-dangle": [0, "never"],
    "no-undef": [0, "never"],
    "react/jsx-filename-extension": [0, "never"],
    "no-underscore-dangle": [0, "never"],
    "func-names": [0, "never"],
    "prefer-destructuring": [0, "never"],
    "no-param-reassign": [0, "never"],
    "guard-for-in": 0,
    "no-restricted-syntax": [2, "WithStatement"],
    "newline-per-chained-call": [2, {
      "ignoreChainWithDepth": 5
    }],
    "space-in-parens": 0,
    "key-spacing": 0,
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "none"
    }],
    "max-len": ["error", {
      "code": 150
    }],
    "padded-blocks": 0,
    "no-continue": 0,
    "import/no-extraneous-dependencies": 0,
    "import/newline-after-import": 0,
    "no-mixed-operators": 0,
    "prefer-template": 0,
    "no-trailing-spaces": [1, {
      "skipBlankLines": true
    }]
  }
}